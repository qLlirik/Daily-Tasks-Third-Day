<Window x:Class="ThirdDay.Windows.EndProjectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ThirdDay.Windows"
        mc:Ignorable="d"
        Title="Квадро - Регистрация конца проекта" FontSize="16" Background="LightSalmon" Height="350" Width="525" WindowStartupLocation="CenterScreen" WindowStyle="None">
    <Grid>
        <Button Click="click_Back" Content="Назад" Width="120" Height="30" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="10"/>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Выбор проекта" TextAlignment="Center" Margin="10"/>
            <ComboBox x:Name="cbxProjects" Width="300" Margin="10"/>
            <Button Click="click_Finishing" Content="Закончен" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
